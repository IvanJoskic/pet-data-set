<!DOCTYPE html>
<html>
<head>
<title><%= title %></title>
<link rel='stylesheet' href='/styles/site.css' />
<style>
    main {
        overflow: auto;
    }
</style>
</head>

<body>
<main>
    <%- include(`partials/header`); %>
    <table>
        <thead>
            <tr>
                <th>Breed name</th>
                <th>Life expectancy</th>
                <th>Weight</th>
                <th>Height</th>
                <th>Temperament</th>
                <th>Colours</th>
                <th>Coat</th>
                <th>Wiki handle</th>
                <th>Description</th>
                <th>Gender</th>
                <th>Country of origin</th>
                <th>Classification</th>
                <th>Species</th>
                <th>Descendant Of</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function (breedData) { %>
            <tr>
                <td><%= breedData.breedname %></td>
                <td><%= breedData.lifeexpectancy %></td>
                <td><%= breedData.weight %></td>
                <td><%= breedData.height%></td>
                <td><%= breedData.temperament%></td>
                <td><%= breedData.colours%></td>
                <td><%= breedData.coat%></td>
                <td><%= breedData.wiki%></td>
                <td><%= breedData.description%></td>
                <td><%= breedData.gender%></td>
                <td><%= breedData.countryoforigin %></td>
                <td><%= breedData.classification%></td>
                <td><%= breedData.species%></td>
                <td><%= breedData.descendantof_breed%></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</main>
</body>
</html>